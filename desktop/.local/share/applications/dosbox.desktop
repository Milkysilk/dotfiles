#!/usr/bin/env xdg-open
[Desktop Entry]
Categories=Application;Game;
Comment=Emulator with builtin DOS for running DOS Games
Exec=bash -c "dosbox -conf $XDG_CONFIG_HOME/dosbox/dosbox.conf"
Icon=dosbox
Name=Dosbox
NoDisplay=false
Path[$e]=
StartupNotify=false
Terminal=0
TerminalOptions=
Type=Application
Version=1.0
X-KDE-SubstituteUID=false
X-KDE-Username=
